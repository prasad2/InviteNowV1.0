<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coen268.invitenow.nishant.invitenowv10" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name=".startScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SendInvitesActivity"
            android:label="@string/title_activity_send_invites"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name=".SelectFromContactsActivity"
            android:label="@string/title_activity_select_from_contacts"
            android:parentActivityName=".SendInvitesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coen268.invitenow.nishant.invitenowv10.SendInvitesActivity" />
        </activity>
        <activity
            android:name=".SelectFromGroupsActivity"
            android:label="@string/title_activity_select_from_groups"
            android:parentActivityName=".SendInvitesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coen268.invitenow.nishant.invitenowv10.SendInvitesActivity" />
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".SendInvitesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coen268.invitenow.nishant.invitenowv10.SendInvitesActivity" />
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile"
            android:parentActivityName=".SettingsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coen268.invitenow.nishant.invitenowv10.SettingsActivity" />
        </activity>
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_edit_profile" >
        </activity>
        <activity
            android:name=".NotificationActivity"
            android:label="@string/title_activity_notification" >
        </activity>
        <activity
            android:name=".LocationPreferenceActivity"
            android:label="@string/title_activity_location_preference" >
        </activity>
        <activity
            android:name=".GroupsActivity"
            android:label="@string/title_activity_groups" >
        </activity>
        <activity
            android:name=".PreferencesActivity"
            android:label="@string/title_activity_preferences" >
        </activity>
    </application>

</manifest>
